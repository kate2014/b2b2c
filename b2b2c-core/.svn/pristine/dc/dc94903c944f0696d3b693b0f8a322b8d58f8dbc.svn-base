<#assign regionSelectDirective="com.enation.app.shop.front.tag.goods.other.regions.RegionSelectDirective"?new()>
<#assign dateformat="com.enation.framework.directive.DateformateDirective"?new()>
<div class="main">
	<div class="detail" style="margin-left: 10px;">
		<table cellspacing="0" cellpadding="0" border="0">
			<tr>
				<td>店铺账号(会员账号)：</td>
				<td>
					<input class="input_text easyui-validatebox" name="member_name"type="text" disabled="disabled" data-options="required:true" value="${uname}"/>
				</td>
			</tr>
			<tr>
				<td>身份证号：</td>
				<td>
					<input class="input_text easyui-validatebox" maxlength="18" type="text" name="id_number" data-options="required:true" value=""></input>
				</td>
			</tr>
			<tr>
				<td>店铺名称：</td>
				<td>
					<input class="input_text easyui-validatebox" maxlength="15" type="text" data-options="required:true" name="store_name" value=""></input>
				</td>
			</tr>
			<tr>
				<td>所在地：</td>
				<td ><@regionSelectDirective  province_name='store_province' city_name='store_city' region_name='store_region'/></td>
			</tr>
			<tr>
				<td>详细地址：</td>
				<td><input class="input_text easyui-validatebox" maxlength="35" type="text" name="attr" value="" data-options="required:true"></input></td>
			</tr>
			<tr>
				<td>邮编：</td>
				<td>
					<input id="zip" name="zip" onkeyup="this.value=this.value.replace(/\D/g,'')"  onafterpaste="this.value=this.value.replace(/\D/g,'')" maxlength="6" class="input_text">
				</td>	
			</tr>
			<tr>
				<td>联系电话：</td>
				<td><input class="input_text easyui-validatebox" maxlength="11" type="text" name="tel" value="" validType="mobile" data-options="required:true"></input></td>
			</tr>
			<tr>
				<td>店铺佣金比例：</td>
				<td >
					<input class="easyui-numberbox input_text" maxlength="10" type="text" max="100" name="commission" data-options="required:true"/>
					如果输入10.平台佣金为订单价格的百分之10,只能输入整数
				</td>
			</tr>
		</table>
	</div>
</div>